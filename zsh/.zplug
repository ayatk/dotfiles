source $DOTDIR/Zplug/zplug

export ZPLUG_HOME="$DOTDIR/plugins"

export PATH=$PATH:$ZPLUG_HOME/bin

zplug "b4b4r07/enhancd", at:v1, as:command, of:enhancd.sh

zplug "b4b4r07/favdir", as:command, of:favdir.sh

if ! zplug check --verbose; then
    printf "Install? [y/N]: "
    if read -q; then
        echo; zplug install
    fi
fi

zplug load --verbose
